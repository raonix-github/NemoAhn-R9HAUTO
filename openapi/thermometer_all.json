            }
          },
          "400": {
            "description": "Invalid ID supplied"
          },
          "404": {
            "description": "Thermometer not found"
          }
        }
      }
    },
    "/thermometers/{deviceId}/heating": {
      "get": {
        "tags": [
          "thermometer"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "$ref": "#/parameters/deviceIdParam"
          },
