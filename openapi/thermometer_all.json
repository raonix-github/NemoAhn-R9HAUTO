  "schemes": [
    "http"
  ],
  "paths": {
    "/thermometers/{deviceId}/settingTemperature": {
      "get": {
        "tags": [
          "thermometer"
        ],
        "summary": "Find thermometer's state by ID",
        "description": "Returns a single thermometer",
        "operationId": "NemoAhnAPIGetThermometer",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "$ref": "#/parameters/deviceIdParam"
          },
          {
            "$ref": "#/parameters/startDateParam"
          },
          {
            "$ref": "#/parameters/endDateParam"
          },
          {
            "$ref": "#/parameters/dataTypeParam"
          },
          {
            "$ref": "#/parameters/intervalParam"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/ThermometerModel"
            }
          },
          "400": {
            "description": "Invalid ID supplied"
          },
          "404": {
            "description": "Thermometer not found"
          }
        }
      }
    },
    "/thermometers/{deviceId}/currentTemperature": {
      "get": {
        "tags": [
